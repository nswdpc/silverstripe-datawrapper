{
    "name": "nswdpc/silverstripe-datawrapper",
    "description": "Module providing Datawrapper chart and map support",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "datawrapper",
        "charts",
        "maps",
        "nsw",
        "iframe",
        "elemental"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Ellis",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        }
    ],
    "require": {
        "nswdpc/silverstripe-elemental-iframe":"^1",
        "codem/silverstripe-html5-inputs": "^1",
        "silverstripe/framework" : "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "autoload": {
        "psr-4": {
            "NSWDPC\\Datawrapper\\": [
                "src/Controllers/"
            ],
            "NSWDPC\\Elemental\\Models\\Datawrapper\\": [
                "src/Models/Elements/"
            ],
            "NSWDPC\\Datawrapper\\Tests\\": [
                "tests/"
            ],
            "NSWDPC\\Elemental\\Tests\\Datawrapper\\": [
                "tests/"
            ]
        }
    },
    "extra": {
        "expose": [
            "client/static"
        ]
    }
}
